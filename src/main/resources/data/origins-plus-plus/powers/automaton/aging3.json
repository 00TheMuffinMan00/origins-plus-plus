{
	"type": "origins:action_over_time",
	"interval": 1,
	"entity_action": {
		"type": "origins:and",
		"actions": [{
				"type": "origins:execute_command",
				"command": "resource change @s origins-plus-plus:automaton/waterbar -1"
			},
			{
				"type": "origins:execute_command",
				"command": "resource change @s origins-plus-plus:automaton/firebar -1"
			}
		]
	},
	"condition": {
		"type": "origins:on_block",
		"inverted": true
	},
	"hidden": true
}