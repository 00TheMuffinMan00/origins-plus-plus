{
	"type":"origins:multiple",
	"resource":{
		"type":"origins:resource",
		"max":400,
		"min":0,
		"start_value":400,
		"hud_render":{
			"sprite_location":"origins:textures/gui/community/huang/resource_bar_01.png",
			"should_render":true,
			"bar_index":5
		}
	},
	"toggle":{
		"type":"origins:toggle",
		"key": "key.saveToolbarActivator",
		"active_by_default":false
	},
	"invulnerability":{
		"type":"origins:invulnerability",
		"damage_condition":{
			"type":"origins:name",
			"name":"inWall"
		}
	},
	"weak":{
		"type":"origins:stacking_status_effect",
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		},
		"duration_per_stack":20,
		"min_stacks":0,
		"max_stacks":1,
		"effect":{
			"duration":0,
			"show_particles":false,
			"show_icon":false,
			"effect":"weakness",
			"amplifier":100
		}
	},
	"timer":{
		"type":"origins:action_over_time",
		"interval":1,
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:power_active",
				"power":"*:*_toggle"
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":-1
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":1
			}
		}
	},
	"wither":{
		"type":"origins:stacking_status_effect",
		"min_stacks":0,
		"max_stacks":1,
		"duration_per_stack":20,
		"effect":{
			"duration":0,
			"show_particles":false,
			"show_icon":false,
			"effect":"wither",
			"amplifier":1
		},
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		}
	},
	"phase":{
		"type":"origins:phasing",
		"blacklist":true,
		"render_type":"blindness",
		"view_distance":20,
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		},
		"block_condition":{
			"type":"origins:in_tag",
			"tag":"origins:unphasable"
		},
		"phase_down_condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:power_active",
					"power":"*:*_toggle"
				},
				{
					"type":"origins:or",
					"conditions":[
						{
							"type":"origins:swimming"
						},
						{
							"type":"origins:sneaking"
						}
					]
				}
			]
		}
	},
	"respawn":{
		"type":"origins:action_on_callback",
		"entity_action_respawned":{
			"type":"origins:change_resource",
			"resource":"*:*_resource",
			"change":500
		}
	},
	"name":"Phase",
	"description":"You can pass through blocks (but it's too unstable to phase for long)."
}